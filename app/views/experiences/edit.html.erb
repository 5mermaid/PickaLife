
<%= simple_form_for @experience do |f|%>
  <%= f.input :photo%>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.input :activity %>
  <%= f.input :place %>
  <%= f.submit "Update", class: "btn btn-primary" %>
<% end %>

<%= simple_form_for @experience, :url => experience_path(@experience), :method => :delete do |f|%>
  <%= f.submit "Destroy", class: "btn btn-primary" %>
<% end %>
