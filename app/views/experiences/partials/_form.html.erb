<%= simple_form_for experience, html: { class: "form"} do |f| %>
  <%= f.input :photo, as: :file %>
  <%= f.input :activity %>
  <%= f.input :start_date, :placeholder => 'Select start Date' %>
  <%= f.input :end_date, :placeholder => 'Select end Date'  %>
  <%= f.input :place %>
  <%= f.input :activity %>
  <%= f.input :price %>
  <%= f.submit %>
<% end %>
